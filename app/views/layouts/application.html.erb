<!DOCTYPE html>
<html>
  <head>
    <title>Serendipity</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  
  </head>

  <body>
    <% if current_user %>
     <%= link_to “Logout”, destroy_user_session_path, method: :delete %>
    <% else %> 
     <%= link_to “Login”, new_user_session_path %>
    <% end %>
    <%= yield %>
  </body>
</html>
